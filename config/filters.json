[
  {
    "ext": ".js",
    "filters": ["entropy.meter"],
    "parser": {
      "module": "tokenizer/js",
      "config": {
        "allowReturnOutsideFunction": true,
        "allowImportExportEverywhere": true,
        "allowHashBang": true
      }
    }
  },
  {
    "ext": ".json",
    "filters": ["entropy.meter"],
    "parser": {
      "module": "tokenizer/json",
      "config": {
        "checkObjectKeys": true,
        "checkObjectValues": true
      }
    }
  }
]
